{
  "family": "brevity",
  "containerDefinitions": [
    {
      "name": "THIS_WILL_BE_REPLACED_BY_GITHUB_ACTIONS",
      "image": "THIS_WILL_BE_REPLACED_BY_GITHUB_ACTIONS",
      "cpu": 128,
      "memory": 256,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "MODE",
          "value": "release"
        },
        {
          "name": "SERVER_PORT",
          "value": "80"
        },
        {
          "name": "OTEL_COLLECTOR_GRPC_URL",
          "value": "http://your-otel-collector-grpc-url:4317"
        },
        {
          "name": "LLM_MODEL_ID",
          "value": "google/gemini-2.5-flash-preview"
        },
        {
          "name": "LLM_BASE_URL",
          "value": "https://openrouter.ai/api/v1/"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "LLM_API_KEY",
          "valueFrom": "arn:aws:ssm:us-east-1:969026361642:parameter/brevity/llm-api-key"
        },
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:ssm:us-east-1:969026361642:parameter/brevity/database-url"
        }
      ],
      "systemControls": []
    }
  ],
  "executionRoleArn": "arn:aws:iam::969026361642:role/brevity-ecs-execution-20250527104533230800000002",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": []
}
